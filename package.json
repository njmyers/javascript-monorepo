	"scripts": {
		"predeploy": "run-p build",
		"deploy": "s3-deploy deploy",
		"s3-deploy": "s3-deploy",
		"build-modernizr": "modernizr -c modernizr-config.json -d src/extra/ -u",
		"prebuild": "run-p build-modernizr",
		"build": "react-scripts build",
		"postbuild": "node ./scripts/post-build.js",
		"prestart": "run-s build-modernizr",
		"start-js": "react-scripts start",
		"start": "run-p start-js",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"precommit": "lint-staged",
		"prettier": "prettier src/**/*.{js,jsx,json,md} --write"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,json,md}": [
			"prettier --write",
			"git add"
		]
	}
