{
    "name": "react-size-components",
    "version": "1.0.24",
    "description": "Higher order components for sizing functions",
    "main": "build/index.js",
    "scripts": {
        "prepublishOnly": "npm run build",
        "watch": "webpack --watch",
        "build": "webpack",
        "test": "jest --watchAll",
        "patch-release": "npm version patch && npm publish && git push --follow-tags",
        "build-readme": "node scripts/readme.js",
        "precommit": "run-s build-readme lint-staged",
        "start": "run-p storybook",
        "deploy-storybook": "storybook-to-ghpages",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "build-ssr": "webpack --config ssr/webpack.config.js",
        "prestart-ssr": "npm run build-ssr",
        "start-ssr": "babel-node ssr/server.js"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,json}": ["prettier --write", "git add"]
    },
    "author": "Nick Myers",
    "license": "ISC",
    "peerDependencies": {
        "lodash": "^4.17.5",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "smalldash": "^1.0.2"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.3.15",
        "@storybook/addon-info": "^3.3.15",
        "@storybook/addon-links": "^3.3.15",
        "@storybook/addon-options": "^3.3.15",
        "@storybook/addons": "^3.3.15",
        "@storybook/react": "^3.3.15",
        "@storybook/storybook-deployer": "^2.3.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-react-docgen": "^1.8.3",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-preset-env": "^1.6.1",
        "husky": "^0.14.3",
        "lint-staged": "^7.0.0",
        "npm-run-all": "^4.1.2",
        "prettier": "^1.11.1",
        "storybook-readme": "^3.3.0",
        "webpack": "^3.0.0",
        "webpack-cli": "^2.0.11"
    },
    "dependencies": {
        "lodash": "^4.17.5",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "smalldash": "^1.0.2"
    }
}
