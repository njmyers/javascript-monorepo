{
  "name": "react-youtube-component",
  "version": "1.0.7",
  "description":
    "Higher order component for downloading and using the YouTube iFrame API",
  "main": "build/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "webpack": "webpack",
    "module": "flow-remove-types src/ -d lib/ -i stories\\|__tests__ -p",
    "build": "run-s webpack module",
    "test": "jest --watchAll",
    "patch-release":
      "npm version patch && npm publish && git push --follow-tags",
    "source-map": "source-map-explorer build/index.js build/index.js.map"
  },
  "author": "Nick Myers",
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "smalldash": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "flow-remove-types": "^1.2.3",
    "jest": "^23.1.0",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-size-components": "^1.3.4",
    "smalldash": "^1.1.6"
  }
}
